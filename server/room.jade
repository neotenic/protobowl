								//- 		<img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1">
								//- 		</form>
								//- 	td Fund Protobowl

								tr
									th latency
									td#latency ?
								tr
									th sync offset
									td#sync_offset ? 
								tr
									td
										a(href="#", onclick="sock.disconnect();return false") Disconnect
									td#cachestatus 
		

		

			
